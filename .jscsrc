{
  "requireCurlyBraces": [
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch"
  ],
  "requireOperatorBeforeLineBreak": true,
  "requireCamelCaseOrUpperCaseIdentifiers": true,
  "maximumLineLength": {
    "value": 90,
    "allowComments": true,
    "allowRegex": true
  },
  "validateIndentation": 4,
  "validateQuoteMarks": "'"
}
